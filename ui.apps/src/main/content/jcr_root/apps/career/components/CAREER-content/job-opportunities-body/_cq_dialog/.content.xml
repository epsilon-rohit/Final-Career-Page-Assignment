<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="job-opportunities-body"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <asset
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}false"/>
                    <metadata
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter the text for job opportunities body"
                                                fieldDescription="text to enter for job opportunities"
                                                fieldLabel="Job opportunities body"
                                                name="./text"
                                                required="{Boolean}true"/>
                                            <text2
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter the text for job opportunities body"
                                                fieldDescription="text to enter for job opportunities body"
                                                fieldLabel="Job opportunities body"
                                                name="./text2"
                                                required="{Boolean}true"/>
                                            <text3
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter the text for job opportunities body"
                                                fieldDescription="text to enter for job opportunities body"
                                                fieldLabel="Job opportunities body"
                                                name="./text3"
                                                required="{Boolean}true"/>
                                            <text4
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter the text for Smart Recruiters"
                                                fieldDescription="text to enter  for Smart Recruiters"
                                                fieldLabel="Smart Recruiters"
                                                name="./text4"
                                                required="{Boolean}true"/>
                                            <text5
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter the text for Linkedin"
                                                fieldDescription="text to enter for for Linkedin"
                                                fieldLabel="Linkedin"
                                                name="./text5"
                                                required="{Boolean}true"/>
                                            <text6
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter The Text"
                                                fieldDescription="Enter The Text"
                                                fieldLabel="Enter The Text"
                                                name="./text6"
                                                required="{Boolean}true"/>
                                            <text7
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Enter The Text"
                                                fieldDescription="Enter The Text"
                                                fieldLabel="Enter The Text"
                                                name="./text7"
                                                required="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
